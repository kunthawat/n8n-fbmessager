{
	"name": "n8n-nodes-facebook-messenger",
	"version": "1.0.0",
	"description": "n8n node for Facebook Messenger integration",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Kunthawat Greethong",
		"email": "kunthawat@moreminimore.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kunthawat/n8n-fbmessager.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/FacebookMessengerApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/FacebookMessenger/FacebookMessenger.node.js",
			"dist/nodes/FacebookMessenger/FacebookMessengerTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/node": "^16.11.7",
		"@types/request-promise-native": "~1.0.15",
		"gulp": "^5.0.0",
		"n8n-workflow": "^1.70.0",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	}
}
